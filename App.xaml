<Application x:Class="GamelistManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GamelistManager"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomRoundedButton.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/themes/DefaultTheme.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomComboBox.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomCheckBox.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomSlider.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomImageButton.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomSplitter.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomRadioButton.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomGroupBox.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomTabControl.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomTextBox.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomMenuItem.xaml"/>
                <ResourceDictionary Source="/Resources/Resource Dictionaries/controls/CustomProgressBar.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>